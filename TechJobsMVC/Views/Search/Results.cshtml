@{
}
@foreach (Job job in ViewBag.jobs)
{
<table class="job-listing">
    <tr>
        <td>
            ID
        </td>
        <td>
            @job.Id
        </td>
    </tr>
    <tr>
        <td>
            Name
        </td>
        <td>
            @job.Name
        </td>
    </tr>
    <tr>
        <td>
            Employer
        </td>
        <td>
            @job.Employer
        </td>
    </tr>
    <tr>
        <td>
            Location
        </td>
        <td>
            @job.Location
        </td>
    </tr>
    <tr>
        <td>
            Skill
        </td>
        <td>
            @job.CoreCompetency
        </td>
    </tr>
</table>
}
